{
  "name": "react-shop-ukrainian",
  "version": "1.0.0-beta.1",
  "description": "Ukrainian messages for React Shop",
  "author": "Vasyl Pukii",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-shop-dev/react-shop.git",
    "directory": "packages/react-shop-ukrainian"
  },
  "keywords": [
    "react-shop",
    "translation"
  ],
  "scripts": {
    "build": "pnpm run build-cjs && pnpm run build-esm",
    "build-cjs": "rimraf ./dist/cjs && tsc --outDir dist/cjs",
    "build-esm": "rimraf ./dist/esm && tsc --outDir dist/esm --module es2015",
    "watch": "tsc --outDir dist/esm --module es2015 --watch"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "react-shop-types": "^1.0.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.5.4"
  }
}
